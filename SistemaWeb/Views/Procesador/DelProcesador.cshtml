@model SistemaWeb.Models.Procesador

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h3>Esta accion NO se puede deshacer. Confirma que estas seguro de Eliminar esta marca del sistema</h3>
<div>
    <h4>Procesador</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Gen)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Gen)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Marca)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Marca)
        </dd>
    </dl>
    
    <form asp-action="DelProcesador">
        <input type="hidden" asp-for="Id" />
        <input type="submit" value="Eliminar Procesador" class="btn btn-danger" /> |
        <a asp-action="ProcesadorIndex">Volver a la Lista</a>
    </form>
</div>
